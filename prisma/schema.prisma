generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  desc      String
  image_url String
  name      String
  repo_url  String
}

model User {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  email     String
  google_id String
}
